{
	"type": "config",
	"id": "conf.service.nextcloud",
	"title": "Nextcloud",
	"queryinfo": {
		"xpath": "//services/nextcloud",
		"iterable": false
	},
	"properties": {
		"enable": {
			"type": "boolean",
			"default": false
		},
		"port": {
			"type": "integer",
			"minimum": 0,
			"maximum": 65535,
			"default": 80
		},
		"ssl_enable": {
			"type": "boolean",
			"default": false
		},
		"sslcertificateref": {
			"type": "string",
			"default": ""
		},
		"sharedfolderref": {
			"type": "string",
			"default": ""
		}
	}
}